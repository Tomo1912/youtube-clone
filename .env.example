# Application Configuration
NODE_ENV=production
PORT=3000

# YouTube API (Optional - for real data)
YOUTUBE_API_KEY=your_youtube_api_key_here

# AWS Configuration (for deployment)
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your_aws_account_id

# Docker Registry
DOCKER_USERNAME=your_dockerhub_username
DOCKER_IMAGE_NAME=youtube-clone

# EKS Configuration
EKS_CLUSTER_NAME=youtube-clone-cluster
EKS_NODE_GROUP_NAME=youtube-clone-nodes

